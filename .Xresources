Xft*dpi:                    82
Xft*autohint:               true 
!Xft*hintstyle:  hintfull
Xft*lcdfilter:  lcddefault
Xft*antialias:              true
Xft*rgba:                   rgb
!Xft.hinting:                true
!Xft.hintstyle:              hintslight


! urxvt config
URxvt*perl-ext-common:  resize-font
URxvt*font:		xft:TerminessTTFNerdFontMono:autohint=false:size=15,xft:DejaVu\ Sans\ Mono\ for\ Powerline:autohint=false:size=15
!URxvt*boldFont: xft:DejaVu\ Sans\ Mono\ for\ Powerline:autohint=false:size=15,xft:TerminessTTFNerdFontMono:autohint=true:size=15
URxvt*scrollBar:                  false
URxvt*mouseWheelScrollPage:       false
URxvt*cursorBlink:                true
URxvt*saveLines:                  5000
URxvt*imLocale: en_US.UTF-8
!URxvt*buffered: false
!URxvt*intensityStyles: false
!URxvt*internalBorder: 5
!URxvt*geometry: 80x24
!URxvt*skipBuiltinGlyphs:    true
!URxvt*linespace:  2
URxvt*letterSpace: 1

! rofi config
rofi.color-enabled: true
rofi.color-window: #1d1e26, #b5bd68, #b5bd68
rofi.color-normal: #1d1e26, #b0b287, #8c9440, #b0b287, #78824B
rofi.color-active: #1d1e26, #b0b287, #8c9440, #b0b287, #78824B
rofi.color-urgent: #1d1e26, #b0b287, #8c9440, #b0b287, #78824B
rofi.separator-style: solid
rofi.sidebar-mode: false
rofi.lines: 5
rofi.font: DejaVuSansMono Nerd Font Bold 9
rofi.bw: 1
rofi.columns: 2
rofi.padding: 5
rofi.fixed-num-lines: true
rofi.hide-scrollbar: true


! st config
st.font:      TerminessTTFNerdFontMono:size=15
!st.bold_font:		0
!st.fontalt0:     xft:DejaVuSansMono Nerd Font:size=13
st.borderpx: 3
!st.background:		#131415
!st.foreground:		#A3BAD0
!st.cursorColor:		#8d9b53
!st.bellvolume:		100
st.cursorBlink:     true
st.tabspaces:		8
st.chscale:			1.0
st.cwscale:			1.0
st.cols:            80
st.alpha: 1



xterm*font:		xft:TerminessTTF Nerd Font Mono:size=15,xft:DejaVu\ Sans\ Mono\ Nerd\ Font:size=15
xterm*scrollBar:                  false
xterm*selectToClipboard:          true
xterm*mouseWheelScrollPage:       false
xterm*cursorBlink:                false
xterm*saveLines:                  5000

